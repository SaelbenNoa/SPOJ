#include <iostream>

using namespace std;

int main () {
	int caseCount;
	cin >> caseCount;
	int cases [caseCount][2];
	for(int i = 0; i < caseCount; i++) {
		cin >> cases[i][0] >> cases[i][1];
	}
	for(int i = 0; i < caseCount; i++) {
		cout << "*";
		for(int z = 0; z < cases[i][1]; z++) {
			cout << "***";
		}
		cout << endl;
		for(int a = 1; a < ((cases[i][0]*3)+1); a++) {
			cout << "*";
			for(int b = 0; b < cases[i][1]; b++) {
				if(a%3==0) {
					cout << "***";
				} else {
					cout << "..*";
				}
			}
			cout << endl;
		}
		cout << endl;
	}
	return 0;
}
