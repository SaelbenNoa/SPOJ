#include <iostream>

using namespace std;

int main () {
	int caseCount;
	cin >> caseCount;
	int cases [caseCount][2];
	for(int i = 0; i < caseCount; i++) {
		cin >> cases[i][0] >> cases[i][1];
	}
	for(int i = 0; i < caseCount; i++) {
		for(int a = 0; a < cases[i][0]; a++) {
			for(int b = 0; b < cases[i][1]; b++) {
				if(a==0 || a==(cases[i][0] - 1) || b==0 || b==(cases[i][1] - 1)) {
					cout << "*";
				} else {
					cout << ".";
				}
			}
			cout << endl;
		}
		cout << endl;
	}
	return 0;
}
